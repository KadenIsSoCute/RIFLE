BlackList = {3088713061, 711472248, 2284392016, 141093789}

if table.find(BlackList, game:GetService("Players").LocalPlayer.UserId) then
    local ohString1 = "You have been blacklisted from using RIFLE"
    local ohString2 = "All"
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(ohString1, ohString2)
    wait(3)
    game.ReplicatedStorage.MainEvent:FireServer("OneMoreTime")
end
